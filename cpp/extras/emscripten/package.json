{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "okiidoku",
	"version": "0.1.0",
	"description": "david fong's variable-grid-size sudoku toolkit",
	"keywords": [
		"sudoku"
	],
	"homepage": "https://github.com/david-fong/okiidoku",
	"license": "AGPL-3.0-or-later",
	"author": "David Fong (https://github.com/david-fong)",
	"main": "okiidoku.js",
	"types": "okiidoku.d.ts",
	"type": "module",
	"files": [],
	"directories": {
		"example": "examples",
		"doc": "doc"
	},
	"repository": "github:david-fong/okiidoku",
	"readme": "README.md",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"emconfigure": "cmake -S ../.. --preset dev.emscripten",
		"embuild.dbg": "cd ../.. && cmake --build --preset dev.emscripten.dbg",
		"embuild.rel": "cd ../.. && cmake --build --preset dev.emscripten.rel",
		"embuild.reldbg": "cd ../.. && cmake --build --preset dev.emscripten.reldbg",
		"eminstall.dbg": "cd ../.. && cmake --install ./out/build/dev.emscripten/ --config Debug",
		"eminstall.rel": "cd ../.. && cmake --install ./out/build/dev.emscripten/ --config Release",
		"eminstall.reldbg": "cd ../.. && cmake --install ./out/build/dev.emscripten/ --config RelWithDebInfo"
	},
	"devDependencies": {
		"typescript": "^5.9.2"
	},
	"engines": {
		"node": ">=16.0.0"
	}
}